<odoo>
    <data>

        <record id="view_product_magento_tree" model="ir.ui.view">
            <field name="name">view.magento.tree</field>
            <field name="model">product.template</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name"/>
                    <field name="sku_no"/>
                    <field name="qty_available"/>
                    <field name="ready_test_qty"/>
                    <field name="magento_qty"/>
                    <field name="magento_test_qty"/>
                </tree>
            </field>
        </record>
        <record id="view_product_magento_form" model="ir.ui.view">
            <field name="name">view.product.brand.form.inherit</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">

                <div name="button_box" position="inside">
                    <button type="action" class="oe_stat_button" icon="fa-play-circle-o">
                        <div class="o_field_widget o_stat_info">
                            <span class="o_stat_value">
                                <field name="magento_qty" widget="statinfo" readonly="1" nolabel="1"/>
                            </span>
                            <span class="o_stat_text">Magento QTY</span>
                        </div>
                    </button>
                    <button type="action" class="oe_stat_button" icon="fa-play-circle-o">
                        <div class="o_field_widget o_stat_info">
                            <span class="o_stat_value">
                                <field name="magento_test_qty" widget="statinfo" readonly="1" nolabel="1"/>
                            </span>
                            <span class="o_stat_text">Magento Test QTY</span>
                        </div>
                    </button>
                </div>
                <xpath expr="//field[@name='barcode']" position="after">
                    <field name='brand_id'/>
                    <field name='sku_no'/>
                    <field name='ready_test_qty' invisible="1"/>
                    <field name='magento_qty' invisible="1"/>
                    <field name='magento_test_qty' invisible="1"/>
                    <field name='e_commerce'/>
                </xpath>
            </field>
        </record>

        <record id="action_magento_products" model="ir.actions.act_window">
            <field name="name">Magento Products</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.template</field>
            <field name="domain">[('e_commerce', '=', True)]</field>
            <field name="view_mode">kanban,tree,form</field>
            <field name="view_ids"
                   eval="[(5, 0, 0),
                   (0, 0, {'view_mode': 'tree', 'view_id': ref('view_product_magento_tree')}),
                   (0, 0, {'view_mode': 'form', 'view_id': ref('view_product_magento_form')}),
                   ]"/>

        </record>
        <record id="action_warehouse_logistics_view" model="ir.actions.act_window">
            <field name="name">Logistics &amp; Warehouse</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sale.order</field>
            <field name="view_mode">tree,kanban,form,calendar,pivot,graph,activity</field>
            <field name="view_ids"
                   eval="[(5, 0, 0),
                   (0, 0, {'view_mode': 'tree', 'view_id': ref('sale.view_quotation_tree_with_onboarding')}),
                   ]"/>

        </record>

        <menuitem id="menu_logistics_warehouse_root" name="Logistics  &amp; Warehouse" sequence="10"/>
        <menuitem id="menu_logistics_warehouse_logistics" name="Logistics  &amp; Warehouse"
                  parent="menu_logistics_warehouse_root" action="action_warehouse_logistics_view" sequence="1"/>
        <menuitem id="menu_magento_products" name="Magento Products"
                  parent="menu_logistics_warehouse_root" action="action_magento_products" sequence="2"/>
    </data>
</odoo>