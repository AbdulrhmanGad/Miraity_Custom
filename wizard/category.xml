<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <act_window id="generate_missing_ategory_sequence"
                name="Generate Missing Sequence"
                binding_model="product.category"
                res_model="wizard.product.category"
                view_mode="form" target="new" />

    <record id="generate_missing_ategory_sequence_view" model="ir.ui.view">
        <field name="name">product.category</field>
        <field name="model">wizard.product.category</field>
        <field name="arch" type="xml">
            <form string="Sent to Magento">
                <div>
                    YOU Will Generate All Missing Sequences
                </div>
                <footer>
                    <button string="Apply" name="action_apply" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>